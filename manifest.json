{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IIITA Click n Connect",
  "version": "1.2",
  "description": "Student's portal to every IIITA content",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "logos/iiitalogo.png",
    "default_popup": "index.html"
  },
  "permissions": ["storage",
    "tabs",
    "notifications",
    "desktopCapture"
  ],
  "web_accessible_resources": [
       
  ],
  "background": {
    "scripts": [
        "JavaScript/background.js"
    ]
},
  "options_ui": {
    "chrome_style": true,
    "page": "options.html",
    "open_in_tab": false
  }
}
